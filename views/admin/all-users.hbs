<section>
    <div class="container mt-5">
        <table class="table">
            <thead>
                <tr>
                    <th scope="col"class="text-center">Name</th>
                    <th scope="col" class="text-center">Email</th>                                           
                    <th></th>
                    
                </tr>
            </thead>
            <tbody>
                {{#each users}}
                <tr>
                    <th scope="row" class="text-center align-middle">{{this.name}}</th>
                    <td class="text-center align-middle">{{this.email}}</td>                                                 
                </tr>
                {{/each}}
            </tbody>
        </table>
    </div>
</section>